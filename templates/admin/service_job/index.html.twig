{% extends 'base.html.twig' %}

{% block title %}ServiceJob index{% endblock %}

{% block body %}
    <h1>ServiceJob index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Job_name</th>
                <th>Description</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for service_job in service_jobs %}
            <tr>
                <td>{{ service_job.id }}</td>
                <td>{{ service_job.jobName }}</td>
                <td>{{ service_job.description }}</td>
                <td>
                    <a href="{{ path('service_job_show', {'id': service_job.id}) }}">show</a>
                    <a href="{{ path('service_job_edit', {'id': service_job.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('service_job_new') }}">Create new</a>
{% endblock %}
